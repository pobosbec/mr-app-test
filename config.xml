<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bosbec.testapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>mr-app-test</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="support@mobileresponse.se" href="https://www.mobileresponse.com">
        Bosbec MobileResponse Team
    </author>
    <platform name="ios">
      <icon src="img/icon.png" width="180" height="180" />
    </platform>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
