<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bosbec.testapp" version="0.2.3c" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>mr-app-test</name>
    <description>
        Another test-app for Bosbec, MobileResponse.
    </description>
    <author email="support@mobileresponse.se" href="http://www.mobileresponse.com">
        Bosbec MobileResponse Team
    </author>
    <platform name="ios">
      <icon src="img/icon.png" width="180" height="180" />
    </platform>
    <content src="index.html" />
    <plugin name="io.litehelpers.cordova.sqlite" spec="0.7.10" source="pgb" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-app-version" spec="0.1.8" source="npm" />
    <plugin name="cordova-plugin-statusbar" spec="2.1.3" source="npm" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
</widget>
